    "import numpy as np\n",
    "### hbar in SI units, J*s\n",
    "hbar = 1.054571e-34 \n",
    "### electron mass in SI units, kg\n",
    "m = 9.109383e-31\n",
    "### Length of the box in SI units, m\n",
    "L = 1e-9\n",
    "### pi\n",
    "pi = np.pi\n",
    "\n",
    "### E1 in SI units\n",
    "E1 = pi**2 * hbar**2/(2 * m * L**2)\n",
    "print(E1, \"J\")"
   ]
